{
  "name": "deGaulle",
  "version": "1.0.0-1",
  "description": "static website generator with attitude, control and verification too",
  "keywords": [
    "SSG",
    "markdown",
    "link checking",
    "permalink",
    "immutable",
    "validation",
    "reachability",
    "SEO"
  ],
  "author": "Ger Hobbelt",
  "repository": "GerHobbelt/deGaulle",
  "license": "MIT",
  "bin": {
    "deGaulle": "src/cli.js"
  },
  "source": "src/index.js",
  "main": "dist/deGaulle.js",
  "module": "dist/deGaulle.mjs",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "make",
    "format": "make prettier",
    "test": "make test",
    "todo": "make todo",
    "clean": "make clean",
    "superclean": "make superclean",
    "doc": "make doc",
    "lint": "make lint",
    "lintfix": "make lintfix",
    "cov": "make report-coverage",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "@gerhobbelt/gitignore-parser": "^0.2.0-8",
    "@gerhobbelt/glob": "7.1.7-5",
    "@gerhobbelt/markdown-it": "11.0.1-43",
    "@gerhobbelt/markdown-it-testgen": "0.1.6-17",
    "@gerhobbelt/nomnom": "1.8.4-30",
    "@gerhobbelt/prismjs": "1.20.1-2",
    "jsdom": "^16.3.0",
    "markdown-it-dirty-dozen": "file:../../js/markdown-it-dirty-dozen"
  },
  "devDependencies": {
    "@gerhobbelt/live-server": "1.3.1-28",
    "@gerhobbelt/prepend-header": "1.0.8-3",
    "ava": "3.11.1",
    "cross-env": "7.0.2",
    "eslint": "7.6.0",
    "microbundle": "0.12.3",
    "nyc": "15.1.0",
    "prettier-standard": "16.4.1"
  }
}
