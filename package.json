{
  "name": "deGaulle",
  "version": "1.0.0-1",
  "description": "static website generator with attitude, control and verification too",
  "keywords": [
    "SSG",
    "markdown",
    "link checking",
    "permalink",
    "immutable",
    "validation",
    "reachability",
    "SEO"
  ],
  "author": "Ger Hobbelt",
  "repository": "GerHobbelt/deGaulle",
  "license": "MIT",
  "bin": {
    "deGaulle": "dist/cli.js"
  },
  "source": "js/src/index.js",
  "main": "dist/deGaulle.js",
  "esmodule": "dist/deGaulle.js",
  "types": "dist/deGaulle.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "make",
    "format": "make prettier",
    "test": "make test",
    "todo": "make todo",
    "clean": "make clean",
    "superclean": "make superclean",
    "doc": "make doc",
    "lint": "make lint",
    "lintfix": "make lintfix",
    "cov": "make report-coverage",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "@gerhobbelt/gitignore-parser": "0.2.0-8",
    "@gerhobbelt/glob": "7.1.7-5",
    "@gerhobbelt/markdown-it": "12.0.4-51",
    "@gerhobbelt/markdown-it-testgen": "0.1.6-17",
    "@gerhobbelt/nomnom": "1.8.4-30",
    "@gerhobbelt/prismjs": "1.20.1-2",
    "jsdom": "16.4.0",
    "markdown-it-dirty-dozen": "file:../../js/markdown-it-dirty-dozen"
  },
  "devDependencies": {
    "@gerhobbelt/live-server": "1.3.1-28",
    "@gerhobbelt/prepend-header": "1.1.0-7",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "ava": "3.15.0",
    "cross-env": "7.0.3",
    "eslint": "7.19.0",
    "microbundle": "0.13.0",
    "nyc": "15.1.0",
    "prettier-standard": "16.4.1",
    "typescript": "4.1.3",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
